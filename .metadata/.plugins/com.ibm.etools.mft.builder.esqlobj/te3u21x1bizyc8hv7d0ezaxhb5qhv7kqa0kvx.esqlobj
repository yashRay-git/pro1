CREATE FILTER MODULE MF_filter_Filter
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE result REFERENCE To Root.XMLNSC;
		
	
		IF result.root ='madhu' THEN
			
		  RETURN TRUE;
		 
		ELSEIF result.root='kumar' THEN 
			RETURN FALSE;
			
			
		ELSE 
			RETURN Unknown;
		END IF;
		RETURN TRUE;
	END;

END MODULE;